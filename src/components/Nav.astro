---
import logo from "../assets/icons/logo.svg";

import navData from "../data/navData";

import Link from "./Link.astro";
---

<nav aria-label="Primary">
  <div class="navbar">
    <a href="/" class="logomark" aria-label="Go home">
      <img src={logo.src} alt="Logo" />
    </a>
    <ul>
      {
        navData.map((item) => (
          <Link
            text={item.name}
            href={item.path}
            style="primary"
            isFilled={false}
          />
        ))
      }
    </ul>
  </div>
</nav>


